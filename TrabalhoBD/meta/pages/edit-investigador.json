{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "heading",
          "name": "pageTitle",
          "size": "H1",
          "text": "Edit Investigador"
        },
        {
          "components": [
            {
              "class": "col-md-9",
              "components": [
                {
                  "name": "closeLabel",
                  "style": {
                    "fontWeight": "bold"
                  },
                  "text": "Item no longer available.",
                  "type": "label",
                  "visible": "${!canEdit}"
                }
              ],
              "name": "col0",
              "type": "col"
            },
            {
              "class": "col-md-3",
              "components": [
                {
                  "buttonStyle": "secondary",
                  "events": {
                    "click": [
                      {
                        "path": "mostrar-investigador",
                        "type": "navigate"
                      }
                    ]
                  },
                  "icon": "",
                  "name": "closeButton",
                  "style": {
                    "marginBottom": "20px"
                  },
                  "text": "Close",
                  "type": "button",
                  "visible": "${!canEdit}"
                }
              ],
              "name": "col1",
              "type": "col"
            }
          ],
          "name": "row0",
          "style": {},
          "type": "row"
        },
        {
          "components": [
            {
              "class": "col-md-9",
              "components": [
                {
                  "name": "label0",
                  "style": {
                    "fontWeight": "bold"
                  },
                  "text": "Another user has made conflicting changes to one or more of the fields you have modified. Please reload.",
                  "type": "label",
                  "visible": "${hasChanges}"
                }
              ],
              "name": "col0",
              "type": "col"
            },
            {
              "class": "col-md-3",
              "components": [
                {
                  "buttonStyle": "secondary",
                  "events": {
                    "click": [
                      {
                        "code": "Trabalho.Reset()",
                        "type": "execute"
                      },
                      {
                        "code": "await this.Load()",
                        "type": "execute"
                      }
                    ]
                  },
                  "icon": "autorenew",
                  "name": "button0",
                  "style": {
                    "marginBottom": "20px"
                  },
                  "text": "Reload",
                  "type": "button",
                  "visible": "${hasChanges}"
                }
              ],
              "name": "col1",
              "type": "col"
            }
          ],
          "name": "row0",
          "style": {},
          "type": "row"
        },
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Nome",
                              "component": "Nome",
                              "name": "NomeLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 50,
                              "type": "textbox",
                              "name": "Nome",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${investigador.Nome}"
                            },
                            {
                              "type": "required-validator",
                              "name": "NomeRequiredValidator",
                              "component": "Nome",
                              "text": "Nome is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Num Funcionario",
                              "component": "Num_Funcionario",
                              "name": "Num_FuncionarioLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int",
                              "type": "numeric",
                              "name": "Num_Funcionario",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${investigador.Num_Funcionario}"
                            },
                            {
                              "type": "required-validator",
                              "name": "Num_FuncionarioRequiredValidator",
                              "component": "Num_Funcionario",
                              "text": "Num_Funcionario is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Orcid",
                              "component": "ORCID",
                              "name": "ORCIDLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 50,
                              "type": "textbox",
                              "name": "ORCID",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${investigador.ORCID}"
                            },
                            {
                              "type": "required-validator",
                              "name": "ORCIDRequiredValidator",
                              "component": "ORCID",
                              "text": "ORCID is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Id Instituicao",
                              "component": "ID_Instituicao",
                              "name": "ID_InstituicaoLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int",
                              "type": "numeric",
                              "name": "ID_Instituicao",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${investigador.ID_Instituicao}"
                            },
                            {
                              "type": "required-validator",
                              "name": "ID_InstituicaoRequiredValidator",
                              "component": "ID_Instituicao",
                              "text": "ID_Instituicao is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Tempo Total",
                              "component": "Tempo_Total",
                              "name": "Tempo_TotalLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int?",
                              "type": "numeric",
                              "name": "Tempo_Total",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${investigador.Tempo_Total}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "path": "mostrar-investigador",
                                    "type": "navigate"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": []
                  },
                  "itemType": "TrabalhoBd.Models.Trabalho.Investigador",
                  "visible": "${investigador != null && canEdit}"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "hasChanges",
        "type": "set",
        "value": "false",
        "valueType": "bool"
      },
      {
        "name": "canEdit",
        "type": "set",
        "value": "true",
        "valueType": "bool"
      }
    ]
  },
  "name": "Edit Investigador",
  "layout": "Edit"
}