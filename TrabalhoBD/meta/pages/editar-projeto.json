    }
  ],
  "events": {
    "load": [
      {
        "name": "hasChanges",
        "type": "set",
        "value": "false",
        "valueType": "bool"
      },
      {
        "name": "canEdit",
        "type": "set",
        "value": "true",
        "valueType": "bool"
      },
      {
        "name": "Trabalho.getProjetoByID",
        "parameters": [
          {
            "name": "ID",
            "value": "Convert.ChangeType(${parameters.ID}, Type.GetTypeCode(typeof(int)))"
          }
        ],
        "then": [
          {
            "name": "projeto",
            "type": "set",
            "value": "${result}",
            "valueType": "TrabalhoBd.Models.Trabalho.Projeto"
          }
        ],
        "error": [],
        "type": "invoke"
      }
    ]
  },
  "name": "Editar Projeto",
  "layout": "Edit"
}