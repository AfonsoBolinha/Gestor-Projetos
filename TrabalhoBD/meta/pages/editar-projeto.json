                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update Projeto",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          },
                          {
                            "name": "hasChanges",
                            "type": "set",
                            "value": "result is DbUpdateConcurrencyException",
                            "valueType": "bool"
                          },
                          {
                            "name": "canEdit",
                            "type": "set",
                            "condition": "!(result is DbUpdateConcurrencyException)",
                            "value": "false",
                            "valueType": "bool"
                          }
                        ],
                        "name": "Trabalho.updateProjeto",
                        "parameters": [
                          {
                            "name": "ID",
                            "value": "Convert.ChangeType(${parameters.ID}, Type.GetTypeCode(typeof(int)))"
                          },
                          {
                            "name": "Projeto",
                            "value": "${projeto}"
                          }
                        ],
                        "then": [
                          {
                            "path": "mostrar-projetos",
                            "type": "navigate",
                            "condition": "result.StatusCode != System.Net.HttpStatusCode.PreconditionFailed"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "TrabalhoBd.Models.Trabalho.Projeto",
                  "data": "${projeto}",
                  "visible": "${projeto != null && canEdit}"
                }
              ]
            }
          ]
        }
