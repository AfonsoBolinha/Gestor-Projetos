{
  "components": [
    {
      "components": [
        {
          "name": "pageTitle",
          "size": "H1",
          "text": "Projetos",
          "type": "heading"
        },
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "events": {
                    "click": [
                      {
                        "path": "add-editar-projeto",
                        "then": [],
                        "type": "navigate"
                      }
                    ]
                  },
                  "icon": "add_circle_outline",
                  "name": "button0",
                  "style": {
                    "marginBottom": "10px"
                  },
                  "text": "Add",
                  "type": "button"
                },
                {
                  "allowFiltering": true,
                  "allowPaging": true,
                  "allowSorting": true,
                  "columns": [
                    {
                      "property": "ID",
                      "title": "ID"
                    },
                    {
                      "property": "Nome",
                      "title": "Nome"
                    },
                    {
                      "property": "Short_Name",
                      "title": "Short Name"
                    },
                    {
                      "property": "Descricao",
                      "title": "Descricao"
                    },
                    {
                      "property": "ID_UC",
                      "title": "ID UC"
                    },
                    {
                      "property": "Max_Cost",
                      "title": "Max Cost"
                    },
                    {
                      "property": "Total_Cost",
                      "title": "Total Cost"
                    },
                    {
                      "property": "DOI",
                      "title": "DOI"
                    },
                    {
                      "property": "State",
                      "title": "State"
                    },
                    {
                      "formatString": "{0:d}",
                      "property": "Data_Init",
                      "title": "Data Init"
                    },
                    {
                      "formatString": "{0:d}",
                      "property": "Data_Fin",
                      "title": "Data Fin"
                    },
                    {
                      "property": "Cientific_Domain",
                      "title": "Cientific Domain"
                    },
                    {
                      "property": "ID_IR",
                      "title": "ID IR"
                    },
                    {
                      "filterable": false,
                      "sortable": false,
                      "template": [
                        {
                          "attributes": [
                            {
                              "name": "@onclick:stopPropagation",
                              "value": true
                            }
                          ],
                          "buttonStyle": "danger",
                          "events": {
                            "click": [
                              {
                                "condition": "await DialogService.Confirm(\"Are you sure you want to delete this record?\") == true",
                                "error": [
                                  {
                                    "detail": "Unable to delete Projeto",
                                    "severity": "error",
                                    "summary": "Error",
                                    "type": "notify"
                                  }
                                ],
                                "name": "Trabalho.deleteProjeto",
                                "parameters": [
                                  {
                                    "name": "ID",
                                    "value": "${data.ID}"
                                  }
                                ],
                                "then": [
                                  {
                                    "code": "await grid0.Reload();",
                                    "condition": "result != null",
                                    "type": "execute"
                                  }
                                ],
                                "type": "invoke"
                              }
                            ]
                          },
                          "icon": "close",
                          "name": "gridDeleteButton",
                          "size": "sm",
                          "text": "",
                          "type": "button"
                        }
                      ],
                      "textAlign": "center",
                      "width": "70px"
                    }
                  ],
                  "data": "${getProjetosResult}",
                  "events": {
                    "loadData": [],
                    "rowSelect": [
                      {
                        "parameters": [
                          {
                            "name": "ID",
                            "value": "${event.ID}"
                          }
                        ],
                        "path": "editar-projeto",
                        "then": [],
                        "type": "navigate"
                      }
                    ]
                  },
                  "filterMode": "advanced",
                  "itemType": "TrabalhoBd.Models.Trabalho.Projeto",
                  "name": "grid0",
                  "title": "",
                  "type": "datagrid"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "name": "Trabalho.getProjetos",
        "parameters": [],
        "then": [
          {
            "name": "getProjetosResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<TrabalhoBd.Models.Trabalho.Projeto>"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "layout": "Edit",
  "name": "Mostrar Projetos"
}